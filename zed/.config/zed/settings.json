{
  "assistant": {
    "default_open_ai_model": "gpt-3.5-turbo",
    "provider": {
      "name": "openai",
      "default_model": "gpt-3.5-turbo"available_models": []
    }
  },
  "tab_bar": {
    "show": true
  },
  "theme": "Kanagawa Wave",
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "FiraCode Nerd Font",
  "relative_line_numbers": true,
  "cursor_blink": false,
  "base_keymap": "VSCode",
  "scrollbar": {
    "show": "never"
  },
  "buffer_line_height": "comfortable",
  "terminal": {
    "font_family": "FiraCode Nerd Font",
    "font_size": 16,
    "line_height": "comfortable"
  },
  "inlay_hints": {
    "enabled": false
  }
}
