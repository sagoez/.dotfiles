{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5-mini"
    },
    "model_parameters": []
  },
  "features": {
    "edit_prediction_provider": "copilot",
  },
  "tab_bar": {
    "show": true,
  },
  "theme": {
    "mode": "light",
    "light": "Catppuccin Latte",
    "dark": "One Dark",
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 14,
  "buffer_font_family": "FiraCode Nerd Font",
  "relative_line_numbers": "enabled",
  "cursor_blink": false,
  "base_keymap": "VSCode",
  "scrollbar": {
    "show": "never",
  },
  "auto_signature_help": true,
  "tabs": {
    "file_icons": true,
  },
  "buffer_line_height": "comfortable",
  "terminal": {
    "font_family": "FiraCode Nerd Font",
    "font_size": 12,
    "line_height": "comfortable",
  },
  "inlay_hints": {
    "enabled": false,
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy",
        },
        "checkOnSave": true,
        "rustfmt": {
          "extraArgs": ["+nightly"],
        },
      },
    },
    "typescript-language-server": {
      "initialization_options": {
        "preferences": {
          "strictNullChecks": true,
          "target": "ES2020",
        },
      },
    },
  },
}
